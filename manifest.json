{
  "manifest_version": 3,
  "name": "Floating Window Extension",
  "version": "1.0",
  "description": "Displays a floating window on specified pages.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"],
      "css": ["main.css"]
    }
  ]
}
